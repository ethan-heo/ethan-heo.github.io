---
import * as blockComponents from "./blocks";
import type {
    NotionBlogBlockType,
    NotionBlogContent,
} from "../../v1/blog/domain/interfaces/notion-blog-block.interface";

const { type, blocks } = Astro.props as {
    type: NotionBlogBlockType;
    blocks: NotionBlogContent[];
};

const Component = blockComponents[type];
---

{Component && <Component blocks={blocks} />}
